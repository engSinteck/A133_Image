		hall_para {
			hall_name = "MH248";
			status = "okay";
			hall_int_port = <&r_pio PL 9 6 1 0xffffffff 0xffffffff>;
		};

		uart0: uart@05000000 {
			uart-supply = <&reg_dcdc1>;
		};
	};

	gpu: gpu@0x01800000 {
			gpu_idle = <0>;
			dvfs_status = <1>;
